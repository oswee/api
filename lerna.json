{
  "packages": [
    "oswee/module/stubs/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "0.3.0",
  "command": {
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    },
    "publish": {
      "conventionalCommits": true,
      "ignoreChanges": [
        ".prettierrc",
        "appveyor.yml",
        "gulpfile.js",
        ".gitignore",
        "CODEOWNERS",
        "*.md",
        "tslint.json"
      ],
      "message": "chore(release): publish"
    }
  },
  "stream": true
}
